{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 0 * * *"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "outToBlob",
      "path": "scraped-reddit-data/{datetime:MM-dd-yy}",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
